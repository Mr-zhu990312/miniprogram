.header{
  width: 100%;
  height: 360rpx;
  display: flex;
  flex-direction: column;
  background-color: #82C6C7;
}
.button_type{
  width: 140rpx;
  height: 140rpx;
  display: flex;
  flex-direction: column;
  border-radius: 50%;
  align-self: center;
  margin-top:120rpx;
}
.user_img{
  width: 140rpx;
  height: 140rpx;
  align-self: center;
}
.function{
  padding-top: 30rpx;
  height: 80rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
}
.function1{
  width: 170rpx;
  height: 70rpx;
  border-radius: 30rpx;
  position: relative;
  margin-left: 60rpx;
  background-color: #F7F7F7;
  display: flex;
  flex-direction: row;
  box-shadow: 3px 3px 3px #E8E8E8;
}
.text{
  color: #9B9AA5;
  margin-left: 15rpx;
  margin-top: 15rpx;
}
.list{
  width: 90%;
  height: 500rpx;
  margin-left: 5%;
}
.liststyle{
  width: 100%;
  height: 70rpx;
  border-radius: 30rpx;
  margin-top: 30rpx;
  background-color: #F7F7F7;
  display: flex;
  flex-direction: row;
   box-shadow: 3px 3px 3px #E8E8E8;
}

.textstyle{
  color: #9B9AA5;
  font-size: 35rpx;
  margin-top: 12rpx;
  margin-left: 40rpx;
}