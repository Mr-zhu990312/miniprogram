    <text class="function_txt {{curIndex === 0 ?'on':''}}" bindtap="bindTap" data-index="0"> 推荐</text>
  </view>
  <view style='width:140rpx;height:80rpx;display:inline-block;background-color:#EAEAEA'>
    <text class="function_txt {{curIndex === 1 ?'on':''}}" bindtap="bindTap" data-index="1"> 防晒</text>
  </view>
  <view style='width:140rpx;height:80rpx;display:inline-block;background-color:#EAEAEA'>
    <text class="function_txt  {{curIndex === 2 ?'on':''}}" bindtap="bindTap" data-index="2"> 补水</text>
